<!DOCTYPE html>
<html>
<head>
  <title>NinetyNineCatsDay1</title>
  <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %>
  <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
  <%= csrf_meta_tags %>
</head>
<body>
<head>
  <%= link_to "Create New User!", new_user_url %>
  <% if logged_in? %>
  <%= link_to "logout", session_url, method: :delete %>
  <%= current_user.user_name %></head>
  <% else %>
  <%= link_to "login", new_session_url, method: :get %>
  <% end %>
<%= yield %>

</body>
</html>
